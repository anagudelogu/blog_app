<section class="users">
  <%= link_to "New Post", new_user_post_path(user_id: @current_user.id)  %>
  <ul class="users__list">
    <% @users.each do |user| %>
    <li>
      <%= link_to(user_path(id: user.id)) do %>
        <%= render "shared/user_card", user: user %>
      <% end %>
    </li>
    <% end %>
  </ul>
</section>